#include <stdio.h>
#define MAXLINE 1000

int getline(char line[], int maxline);
int removeline(char s[]);

int main()
{
    char line[MAXLINE];

    while (getline(line, MAXLINE) > 0)
        if (removeline(line) > 0)
        printf("%s", line);
        
    return 0;
}

int remove (char[s])

{
    int i;
    i = 0;
    
    while (s[i] != '\n')
        ++i;
    while (i >= 0 && (s[i] == '\t' || s[i] == ' '))
        --i;
    if (i > 0){
    ++i;
    s[i] = '\n';
    ++i;
    s[i] = '\0';
    }
    return i;
}
